{
    "name": "@siux/react-components",
    "version": "0.0.2-test-1.1",
    "description": "Siux.studio React components library",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "preversion": "cd .. && npm run build-library"
    },
    "main": "./index.js",
    "module": "./es/components/index.js",
    "sideEffects": false,
    "types": "./components/index.d.ts",
    "typesVersions": {
        "*": {
            "utils": [
                "es/utils/utils.d.ts"
            ]
        }
    },
    "author": "Julian Schuster",
    "license": "ISC",
    "exports": {
        ".": {
            "import": "./es/components/index.js",
            "types": "./index.d.ts",
            "default": "./index.js"
        },
        "./es": "./es/components/index.js",
        "./es/*": "./es/components/**/*.js",
        "./utils": {
            "import": "./es/utils/utils",
            "types": "./es/utils/utils.d.ts",
            "default": "./utils.js"
        }
    },
    "keywords": [],
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "tslib": "^2.4.0"
    }
}
